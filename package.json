{
  "name": "qiwi-sdk",
  "version": "3.2.0",
  "description": "Typed QIWI Wallet SDK. Supported API's: Personal & P2P Bill Payments",
  "keywords": [
    "qiwi",
    "wallet",
    "p2p",
    "payments",
    "nodejs",
    "typed",
    "api",
    "sdk"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "type": "commonjs",
  "types": "./dist/cjs/index.d.ts",
  "typings": "./dist/cjs/index.d.ts",
  "funding": "https://qiwi.com/n/ALEXXGRIB",
  "files": [
    "dist",
    "docs",
    "examples",
    "README.md",
    "README.en.md",
    "CONTRIBUTING.md",
    "CHANGELOG.md",
    "SECURITY.md",
    "CODE_OF_CONDUCT.md"
  ],
  "directories": {
    "doc": "./docs",
    "lib": "./dist",
    "test": "./src/__tests__",
    "example": "./examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexXanderGrib/node-qiwi-sdk.git"
  },
  "author": {
    "name": "Alexander Gribochkin",
    "email": "me@alexxgrib.me",
    "url": "https://alexxgrib.me"
  },
  "maintainers": [
    {
      "name": "Alexander Gribochkin",
      "email": "me@alexxgrib.me",
      "url": "https://alexxgrib.me"
    }
  ],
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/AlexXanderGrib/node-qiwi-sdk/issues",
    "email": "me@alexxgrib.me"
  },
  "homepage": "https://github.com/AlexXanderGrib/node-qiwi-sdk#readme",
  "scripts": {
    "test": "jest --runInBand",
    "build:dist": "rimraf dist && rollup -c",
    "build:docs": "typedoc --out docs/api src/index.ts",
    "build": "npm run build:dist && npm run build:docs",
    "prepack": "npm run build",
    "prepublishOnly": "npm run generate-lock-files",
    "generate-lock-files": "npm i && yarn install",
    "glf": "npm run generate-lock-files",
    "lint": "eslint src/*",
    "prettify": "prettier --write src/**/*"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "query-string": "^7.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^4.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.24",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "express": "^4.17.3",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.15",
    "typedoc-plugin-markdown": "^3.12.0",
    "typedoc-plugin-missing-exports": "^0.22.6",
    "typescript": "^4.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run prettify",
      "pre-push": "npm run docs"
    }
  },
  "engines": {
    "node": ">10"
  }
}
