{
  "name": "qiwi-sdk",
  "version": "4.1.2",
  "description": "⭐ QIWI SDK for node: P2P Payments & Wallet management",
  "keywords": [
    "qiwi",
    "wallet",
    "p2p",
    "payments",
    "rest",
    "api",
    "sdk",
    "typed",
    "typescript",
    "node",
    "nodejs",
    "backend"
  ],
  "files": [
    "dist/",
    "examples/",
    "coverage/",
    "docs/",
    "src/",
    "index.js",
    "index.mjs",
    "index.d.ts",
    "wallet.js",
    "wallet.mjs",
    "wallet.d.ts",
    "p2p.js",
    "p2p.mjs",
    "p2p.d.ts",
    "detector.js",
    "detector.mjs",
    "detector.d.ts",
    "shared.js",
    "shared.mjs",
    "shared.d.ts",
    "README.md",
    "README.ru.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "LICENSE.txt",
    "SECURITY.md",
    "CODE_OF_CONDUCT.md"
  ],
  "main": "./index.js",
  "module": "./index.mjs",
  "type": "commonjs",
  "types": "./index.d.ts",
  "typings": "./index.d.ts",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./wallet": {
      "require": "./wallet.js",
      "import": "./wallet.mjs",
      "types": "./wallet.d.ts"
    },
    "./p2p": {
      "require": "./p2p.js",
      "import": "./p2p.mjs",
      "types": "./p2p.d.ts"
    },
    "./detector": {
      "require": "./detector.js",
      "import": "./detector.mjs",
      "types": "./detector.d.ts"
    },
    "./shared": {
      "require": "./shared.js",
      "import": "./shared.mjs",
      "types": "./shared.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "wallet": [
        "./wallet.d.ts"
      ],
      "p2p": [
        "./p2p.d.ts"
      ],
      "detector": [
        "./detector.d.ts"
      ],
      "shared": [
        "./shared.d.ts"
      ]
    }
  },
  "directories": {
    "doc": "./docs",
    "lib": "./dist",
    "test": "./src/__tests__",
    "example": "./examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexXanderGrib/node-qiwi-sdk.git"
  },
  "author": {
    "name": "Alexander Gribochkin",
    "email": "me@alexxgrib.me",
    "url": "https://alexxgrib.me"
  },
  "maintainers": [
    {
      "name": "Alexander Gribochkin",
      "email": "me@alexxgrib.me",
      "url": "https://alexxgrib.me"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexXanderGrib/node-qiwi-sdk/issues",
    "email": "me@alexxgrib.me"
  },
  "homepage": "https://github.com/AlexXanderGrib/node-qiwi-sdk#readme",
  "scripts": {
    "test": "jest --runInBand",
    "build:dist": "rimraf dist && rollup -c",
    "build:docs": "typedoc && cp ./README.ru.md ./docs/api/",
    "build": "npm run build:dist && npm run build:docs",
    "prepack": "npm run build",
    "lint": "eslint src/*",
    "prettify": "prettier --write src/**/*"
  },
  "dependencies": {
    "axios": "^1.6.3"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^4.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.35.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-unicorn": "^45.0.2",
    "express": "^4.18.2",
    "glob": "^8.1.0",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "rollup": "^3.17.3",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-prettier": "^3.0.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.25",
    "typedoc-plugin-markdown": "^3.14.0",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=12"
  }
}
