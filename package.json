{
  "name": "qiwi-sdk",
  "version": "1.7.2",
  "description": "Typed, Promise based, QIWI API (P2P & Personal) client",
  "main": "dist/index.js",
  "type": "commonjs",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "directories": {
    "doc": "./docs",
    "lib": "./dist",
    "test": "./src/__tests__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexXanderGrib/node-qiwi-sdk.git"
  },
  "author": "AlexXanderGrib <me@alexxgrib.me> (https://alexxgrib.me)",
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/AlexXanderGrib/node-qiwi-sdk/issues",
    "email": "me@alexxgrib.me"
  },
  "homepage": "https://github.com/AlexXanderGrib/node-qiwi-sdk#readme",
  "scripts": {
    "test": "jest --runInBand",
    "build": "rimraf dist && tsc --project tsconfig.build.json",
    "prepack": "npm run build",
    "prepublishOnly": "npm run generate-lock-files",
    "generate-lock-files": "npm i && yarn install",
    "glf": "npm run generate-lock-files",
    "lint": "eslint src/*",
    "prettify": "prettier --write src/**/*"
  },
  "keywords": [
    "qiwi",
    "wallet",
    "api",
    "sdk",
    "payments",
    "qiwi personal",
    "qiwi p2p",
    "киви",
    "typed"
  ],
  "dependencies": {
    "abort-controller": "^3.0.0",
    "node-fetch": "^2.6.1",
    "query-string": "^7.0.1",
    "sweet-decorators": "^1.8.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "@types/node-fetch": "^2.5.12",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run prettify",
      "pre-push": "npm test"
    }
  },
  "readme": "./README.md",
  "engines": {
    "node": ">10"
  }
}
